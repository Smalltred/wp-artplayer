msgstr "用户ID"

#: inc/class/DanmulistTable.php:67
msgid "Danmuku Content"
msgstr "弹幕内容"

#: inc/class/DanmulistTable.php:68
msgid "Danmuku Time"
msgstr "弹幕时间"

#: inc/class/DanmulistTable.php:69
msgid "Danmuku Send time"
msgstr "发送时间"

#: inc/class/DanmulistTable.php:192 inc/class/DanmulistTable.php:221
msgid "Delete"
msgstr "删除"

#: inc/class/DanmulistTable.php:215
msgid "Restore"
msgstr "恢复"

#: inc/class/DanmulistTable.php:216
msgid "Delete Permanently"
msgstr "永久删除"

#: inc/class/DanmulistTable.php:218
msgid "Move to Trash"
msgstr "移到回收站"

#: inc/class/DanmulistTable.php:225
msgid "Attach"
msgstr "附件"

#: inc/class/DanmulistTable.php:235 inc/class/DanmulistTable.php:243
